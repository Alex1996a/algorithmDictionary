#include <stdio.h>
#include <stdio.h>

int n;

void SelectSort(int *array)
{
	int i,j,k,temp;
	for( i = 0; i < n; i++)
	{
		k = i;
		for(j = i + 1;j < n;j ++)
		{
			if(array[j] < array[k])
			{
				k = j;
			}
		}
		if ( k != i)
		{
			remp = array[i];
			}
		}
}

