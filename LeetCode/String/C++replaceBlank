8-5
lintcode Space Replacement

int replaceBlank(char string[],int length){
	int n = 0;
	for (int i=0; i < length;i++)
		if (string[i] ==' ') n++;
	
	int new_len = length +n*2
	for (int i = length-1; i>=0;i--){
		if(string[i] !=' '){
			string[--new_len] = string[i];
		}eles{
			string[--new_len] = '0';
			string[--new_len] = '2';
			string[--new_len] = '%';
			}
		}
		return length +n*2;
}

