10-30

class Solution:

	# @param {int[]} A an integer array
	# @return {long} a long integer

	def permutataionIndex(self, A):

		if A is None or len(A) == 0:

			return 0 


		index = 1
		factor = 1

		for i in xrange(len(A)) -1, -1, -1):

			rank = 0

			for j in xrange(i + 1, len(A)):

				if A[i] > A[j]:

					rank += 1

			index += rank * factor
			factor *= (len(A) - i) 


		return index


