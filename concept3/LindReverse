8-27

class ListNode:
	def __init__(self, val):
		self.val = val
		self.next = None

	#in Python next is a reversed word 

	def reverse(self, head):
	
		prev = None
		while head:
			temp = head.next
			head.next = prev
			prev = head
			head = temp
		
		return prev


double List Node:

Python:

class DListNode:
	def __init__(self, val):
		self.val = val 
		self.prev = self.next = null

	def reverse(self, head)

	curt = None
	while head:
		curt = head
		head = curt.next
		curt.next = curt.prev
		curt.prev = head
	return curt

	
